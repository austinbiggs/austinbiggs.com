{
  "name": "austinbiggs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "codegen": "graphql-codegen",
    "codegen:watch": "graphql-codegen --watch",
    "dev": "next dev",
    "format": "prettier --write .",
    "legacy:sync:graphql": "yarn --silent gq HASURA_ENDPOINT -H \"X-Hasura-Admin-Secret: HASURA_ADMIN_SECRET\" --introspect > gql/schema.graphql",
    "legacy:sync:graphql:json": "yarn --silent gq HASURA_ENDPOINT -H \"X-Hasura-Admin-Secret: HASURA_ADMIN_SECRET\" --introspect --format json > gql/schema.json",
    "legacy:sync:schema:json": "node ./hooks/sync/graphqurl/schema-as-json.js",
    "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "predev": "yarn codegen",
    "prestart": "yarn codegen",
    "postinstall": "husky install",
    "start": "next start",
    "sync": "node ./hooks/sync && yarn prettier --write ./gql > /dev/null",
    "sync:metadata": "node ./hooks/sync/metadata.js",
    "sync:postgres": "node ./hooks/sync/postgres.js",
    "sync:schema:gql": "node ./hooks/sync/graphqurl/schema-as-gql.js",
    "test": "jest --watch",
    "test:all": "yarn lint && yarn type:check && yarn test",
    "type:check": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "@analytics/segment": "^1.1.1",
    "@apollo/client": "^3.4.17",
    "@bugsnag/js": "^7.13.2",
    "@bugsnag/plugin-react": "^7.13.2",
    "@editorjs/checklist": "^1.3.0",
    "@editorjs/code": "^2.7.0",
    "@editorjs/delimiter": "^1.2.0",
    "@editorjs/embed": "^2.4.6",
    "@editorjs/header": "^2.6.1",
    "@editorjs/image": "^2.7.0",
    "@editorjs/inline-code": "^1.3.1",
    "@editorjs/link": "^2.3.1",
    "@editorjs/list": "^1.6.2",
    "@editorjs/marker": "^1.2.2",
    "@editorjs/quote": "^2.4.0",
    "@editorjs/raw": "^2.3.0",
    "@editorjs/simple-image": "^1.4.1",
    "@editorjs/table": "^2.0.1",
    "@editorjs/warning": "^1.2.0",
    "@fontsource/ubuntu": "^4.5.0",
    "@fortawesome/fontawesome-common-types": "^0.2.32",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/lodash": "^4.14.176",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "analytics": "^0.7.17",
    "babel-plugin-add-react-displayname": "^0.0.5",
    "bootswatch": "^5.1.3",
    "change-case": "^4.1.2",
    "classnames": "^2.3.1",
    "eslint-plugin-no-loops": "^0.3.0",
    "graphql": "^15.6.1",
    "graphql-tools": "^8.2.0",
    "lodash": "^4.17.21",
    "logrocket": "^2.1.1",
    "logrocket-react": "^4.0.1",
    "next": "12.0.2",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.19.4",
    "react-i18next": "^11.12.0",
    "react-use": "^17.3.1",
    "sass": "^1.43.3",
    "serialize-javascript": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@editorjs/editorjs": "^2.22.2",
    "@editorjs/paragraph": "^2.8.0",
    "@graphql-codegen/add": "^3.0.0",
    "@graphql-codegen/cli": "^2.2.1",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/near-operation-file-preset": "^2.1.6",
    "@graphql-codegen/typescript": "^2.2.4",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.1.6",
    "@graphql-codegen/typescript-operations": "^2.1.8",
    "@graphql-codegen/typescript-react-apollo": "^3.1.6",
    "@types/classnames": "^2.3.1",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "apollo-codegen": "^0.20.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.3.1",
    "chai": "^4.3.4",
    "cypress": "^9.0.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-i18next": "^5.1.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-testing-library": "^5.0.0",
    "faker": "^5.5.3",
    "graphqurl": "^1.0.1",
    "husky": "7",
    "i18next": "^21.4.2",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.5",
    "node": "^17.0.1",
    "prettier": "2.4.1",
    "react-editor-js": "^1.10.0",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "stylelint": "^14.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^23.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "graphql": {
    "documents": "./frontend/components/**/*.graphql",
    "schema": "gql/schema.graphql"
  },
  "lint-staged": {
    "*.{gql,graphql,js,json,md,ts,tsx,yml,yaml}": [
      "yarn lint",
      "yarn format",
      "git add"
    ],
    "*.scss": [
      "stylelint --config .stylelintrc.json --syntax scss"
    ]
  },
  "main": "index.js",
  "repository": "git@github.com:austinbiggs/austinbiggs.com.git",
  "author": "Austin Biggs <austintbiggs@gmail.com>",
  "license": "MIT"
}
